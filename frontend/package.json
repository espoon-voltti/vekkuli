{
  "name": "vekkuli-frontend",
  "version": "0.0.1",
  "description": "Frontend for Vekkuli",
  "private": true,
  "license": "LGPL-2.1-or-later",
  "scripts": {
    "clean": "rm -rf dist node_modules/.cache",
    "dev": "concurrently -n tsc,esbuild -c blue,green 'yarn type-check:watch' 'yarn build:serve'",
    "lint": "eslint --max-warnings 0 src",
    "type-check": "tsc --build --force .",
    "type-check:watch": "yarn type-check --watch --preserveWatchOutput",
    "build": "node build.js",
    "build:dev": "node build.js --dev",
    "build:serve": "node build.js --dev --watch --serve"
  },
  "dependencies": {
    "@tanstack/react-query": "5.62.3",
    "@types/axios": "0.14.4",
    "@types/date-fns": "2.6.3",
    "axios": "1.8.4",
    "bulma": "1.0.4",
    "classnames": "2.5.1",
    "compute-scroll-into-view": "3.1.1",
    "core-js": "3.39.0",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "esbuild-sass-plugin": "3.3.1",
    "history": "5.3.0",
    "lodash": "4.17.21",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-focus-on": "3.9.4",
    "react-router": "7.5.2",
    "sass": "1.82.0",
    "sass-loader": "16.0.5",
    "seamless-scroll-polyfill": "2.3.4"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@eslint/compat": "1.2.9",
    "@eslint/js": "9.16.0",
    "@playwright/test": "1.49.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.17",
    "@types/node": "22.10.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "9.2.1",
    "concurrently": "9.1.2",
    "css-loader": "7.1.2",
    "csstype": "3.1.3",
    "esbuild": "0.25.5",
    "eslint": "9.16.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-expressions": "1.3.2",
    "eslint-plugin-lodash": "8.0.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.1.0",
    "express": "4.21.2",
    "express-http-proxy": "2.1.1",
    "globals": "15.13.0",
    "html-webpack-plugin": "5.6.3",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-junit": "16.0.0",
    "lib-common": "link:src/lib-common",
    "lib-customizations": "link:src/lib-customizations",
    "postcss": "8.4.49",
    "postcss-loader": "8.1.1",
    "postcss-preset-env": "10.1.1",
    "prettier": "3.4.2",
    "style-loader": "4.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths-webpack-plugin": "4.2.0",
    "typescript": "5.7.2",
    "typescript-eslint": "8.17.0",
    "webpack": "5.96.1",
    "webpack-cli": "5.1.4",
    "webpack-pwa-manifest": "4.3.0",
    "yargs": "17.7.2"
  },
  "engines": {
    "node": ">= 22.14.0"
  },
  "browserslist": [
    "Firefox ESR",
    "last 2 Chrome versions",
    "last 2 Android versions",
    "last 2 FirefoxAndroid versions",
    "last 2 ChromeAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Samsung versions",
    "> 5% in FI",
    "last 3 years"
  ],
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "projects": [
      "./src/citizen-frontend",
      "./eslint-plugin"
    ]
  },
  "jest-junit": {
    "outputDirectory": "test-results/",
    "addFileAttribute": "true"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": true
    }
  },
  "packageManager": "yarn@4.5.3"
}
