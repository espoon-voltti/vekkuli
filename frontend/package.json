{
  "name": "vekkuli-frontend",
  "version": "0.0.1",
  "description": "Frontend for Vekkuli",
  "private": true,
  "license": "LGPL-2.1-or-later",
  "scripts": {
    "clean": "rm -rf dist node_modules/.cache",
    "dev": "concurrently -n tsc,esbuild -c blue,green 'yarn type-check:watch' 'yarn build:serve'",
    "lint": "eslint --max-warnings 0 src",
    "type-check": "tsc --build --force .",
    "type-check:watch": "yarn type-check --watch --preserveWatchOutput",
    "build": "node build.js",
    "build:dev": "node build.js --dev",
    "build:serve": "node build.js --dev --watch --serve"
  },
  "dependencies": {
    "@tanstack/react-query": "5.90.2",
    "@types/axios": "0.14.4",
    "@types/date-fns": "2.6.3",
    "axios": "1.12.2",
    "bulma": "1.0.4",
    "classnames": "2.5.1",
    "compute-scroll-into-view": "3.1.1",
    "core-js": "3.45.1",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "esbuild-sass-plugin": "3.3.1",
    "history": "5.3.0",
    "lodash": "4.17.21",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-focus-on": "3.10.0",
    "react-router": "7.9.3",
    "sass": "1.93.2",
    "sass-loader": "16.0.5",
    "seamless-scroll-polyfill": "2.3.4"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@eslint/compat": "1.4.0",
    "@eslint/js": "9.36.0",
    "@playwright/test": "1.55.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@types/node": "22.18.8",
    "@types/react": "18.3.25",
    "@types/react-dom": "18.3.7",
    "babel-loader": "10.0.0",
    "concurrently": "9.2.1",
    "css-loader": "7.1.2",
    "csstype": "3.1.3",
    "esbuild": "0.25.10",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-expressions": "1.3.2",
    "eslint-plugin-lodash": "8.0.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "express": "5.1.0",
    "express-http-proxy": "2.1.1",
    "globals": "16.4.0",
    "html-webpack-plugin": "5.6.4",
    "jest": "30.2.0",
    "jest-circus": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-environment-node": "30.2.0",
    "jest-junit": "16.0.0",
    "lib-common": "link:src/lib-common",
    "lib-customizations": "link:src/lib-customizations",
    "postcss": "8.5.6",
    "postcss-loader": "8.2.0",
    "postcss-preset-env": "10.4.0",
    "prettier": "3.6.2",
    "style-loader": "4.0.0",
    "ts-jest": "29.4.4",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths-webpack-plugin": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.44.1",
    "webpack": "5.102.0",
    "webpack-cli": "6.0.1",
    "webpack-pwa-manifest": "4.3.0",
    "yargs": "18.0.0"
  },
  "engines": {
    "node": ">= 22.14.0"
  },
  "browserslist": [
    "Firefox ESR",
    "last 2 Chrome versions",
    "last 2 Android versions",
    "last 2 FirefoxAndroid versions",
    "last 2 ChromeAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Samsung versions",
    "> 5% in FI",
    "last 3 years"
  ],
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "projects": [
      "./src/citizen-frontend",
      "./eslint-plugin"
    ]
  },
  "jest-junit": {
    "outputDirectory": "test-results/",
    "addFileAttribute": "true"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": true
    }
  },
  "packageManager": "yarn@4.10.3"
}
